{
  "building_name": "dalgas",
  "font_size_ranges": [
    {
      "min_size": 3.2,
      "max_size": 3.6
    },
    {
      "min_size": 49.0,
      "max_size": 49.4
    },
    {
      "min_size": 10.0,
      "max_size": 11.0
    }
  ],
  "room_patterns": [
    {
      "pattern": "^[A-Z0-9]{1,4}[-._][A-Z0-9]{1,4}",
      "description": "Format som PH-D1, A-01",
      "enabled": true
    },
    {
      "pattern": "^\\d{2}_\\d{2}$",
      "description": "Format som 01_02",
      "enabled": true
    },
    {
      "pattern": "^[A-Z]\\.\\d\\.\\d{2}$",
      "description": "Format som A.1.01",
      "enabled": true
    },
    {
      "pattern": "^PH-D\\d+\\.?\\d*_?\\d*$",
      "description": "Format som PH-D1.11_01",
      "enabled": true
    },
    {
      "pattern": "^[A-Z]{1,2}\\d{2,4}$",
      "description": "Format som A101, AB123",
      "enabled": true
    },
    {
      "pattern": "^\\d{2,4}[A-Z]?$",
      "description": "Format som 101, 202A",
      "enabled": true
    },
    {
      "pattern": "^[A-Z0-9]{2,8}$",
      "description": "Korte alfanumeriske koder",
      "enabled": true
    },
    {
      "pattern": "^[A-Z0-9.-_]{2,10}$",
      "description": "Generelle alfanumeriske kombinationer",
      "enabled": true
    }
  ],
  "entrance_keywords": [
    "Indgang",
    "entrance",
    "entry",
    "Indgang D"
  ],
  "exclude_patterns": [
    {
      "pattern": "^\\d+\\.\\d+m2$",
      "description": "Arealm√•l",
      "enabled": true
    },
    {
      "pattern": "^(Area|Type|Room \\d+\\.\\d+m2):",
      "description": "Metadata",
      "enabled": true
    },
    {
      "pattern": "^\\d+\\.\\d+$",
      "description": "Lange decimal tal (over 6 cifre)",
      "enabled": true
    },
    {
      "pattern": "^(width|height|scale|rotation|metadata|properties)$",
      "description": "Tekniske termer",
      "enabled": true
    },
    {
      "pattern": "^[\\d.]+$",
      "description": "Kun tal og punktummer",
      "enabled": true
    }
  ],
  "min_text_length": 1,
  "max_text_length": 15,
  "case_sensitive": false
}